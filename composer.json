{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.0.0",
        "barryvdh/laravel-cors": "^2.0",
        "calebporzio/parental": "0.11.0",
        "fideloper/proxy": "^4.4.0",
        "guzzlehttp/guzzle": "^7.0.1",
        "laravel/framework": "^8.0",
        "laravel/horizon": "^5.2.1",
        "laravel/legacy-factories": "^1.3.0",
        "laravel/tinker": "^2.7.1",
        "laravel/ui": "^3.0",
        "nwidart/laravel-modules": "^8.0",
        "owen-it/laravel-auditing": "^13.0.2",
        "php-open-source-saver/jwt-auth": "^1.4",
        "predis/predis": "^1.1",
        "prettus/l5-repository": "^2.7.0",
        "spatie/laravel-ignition": "^1.0",
        "spatie/laravel-permission": "^5.5.2"
    },
    "require-dev": {
        "beyondcode/laravel-dump-server": "^1.5.0",
        "filp/whoops": "^2.7.3",
        "fzaninotto/faker": "^1.4",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^5.0",
        "nunomaduro/larastan": "^1.0.0",
        "phpunit/phpunit": "^9.0",
        "sempro/phpunit-pretty-print": "^1.3.0",
        "spatie/phpunit-watcher": "^1.22"
    },
    "repositories": [
        {
            "type": "path",
            "url": "packages/hello"
        }
    ],
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Modules\\": "Modules/"
        },
        "classmap": [
            "database/seeds",
            "database/factories"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "code:analysis": [
            "phpstan analyse"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }
}
